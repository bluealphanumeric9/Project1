let raceNumber = Math.floor(Math.random() * 1000);

let registeredEarly = true;

let runnerAge = 33;

if(registeredEarly === true && runnerAge > 18){
  raceNumber +=1000;
}

if (registeredEarly === true && runnerAge>18){
console.log(`${raceNumber} will race at 9:30 am`);
}
else if (registeredEarly === true && runnerAge<18){
console.log(`${raceNumber} will race at 11:00 am`);
}
else if (runnerAge<18){
console.log('Youth registrants run at 12:30 pm (regardless of registration)');
}
else {
  console.log('Visit the registration desk');
}


//This algorithm tells to registered runners when their run will start, provided they are at least 18 years old, otherwise to visit the help desk. 
